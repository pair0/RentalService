            <script type="text/javascript" src="//dapi.kakao.com/v2/maps/sdk.js?appkey=24c1964f83338709346c0e7364a5d3ff&libraries=services"></script>
            <script>
                // 주소-좌표 변환 객체를 생성합니다
                var geocoder = new kakao.maps.services.Geocoder();
                var lat, long;
                let id = '<%-id%>'
                let addr = '<%-result.store_address%>'
                // 주소로 좌표를 검색합니다
                geocoder.addressSearch(addr, function(result, status) {
                    // 정상적으로 검색이 완료됐으면 
                    if (status === kakao.maps.services.Status.OK) {
                        lat = result[0].y; // 위도
                        long = result[0].x; // 경도                        
                    }
                });
                function goAllow(){
                    location.href='/user-allow?id=' + '<%-id%>' + '&lat=' + lat + '&long=' + long;
                }
            </script>
 

            
            

